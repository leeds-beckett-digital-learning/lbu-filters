<?xml version="1.0" encoding="UTF-8"?>
<tokenise>
  <delimeter value=""/>
  <delimeter value=""/>
  <delimeter value=""/>
  <switch>
    <when match="is an entry">
      <split spec="split on first =">
        <index from="1" to="1">
          <copy/>
        </index>
        <index from="2" to="2">
          <sequence>
            <replace match="" with=""/>
          </sequence>
        </index>
        <index from="3">
          <copy/>   
        </index>
      </split>
    </when>
    <otherwise>
      <copy/>
    </otherwise>
  </switch>
</tokenise>
